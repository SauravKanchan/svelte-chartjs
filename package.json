{
  "name": "svelte-chartjs",
  "version": "1.1.1",
  "author": "Saurav Kanchan",
  "homepage": "https://saurav.tech/mdbsvelte/?path=/story/charts--examples",
  "license": "MIT",
  "module": "lib/index.mjs",
  "main": "src/index.js",
  "publishConfig": {
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "unpkg": "./dist/umd.js",
    "directory": "package"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "svelte",
    "chartjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SauravKanchan/svelte-chartjs.git"
  },
  "bugs": {
    "url": "https://github.com/SauravKanchan/svelte-chartjs"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "pnpm test && pnpm build && del ./package && clean-publish",
    "postpublish": "del ./package",
    "commit": "cz",
    "release": "standard-version",
    "format": "prettier --write 'src/**/*.{js,svelte}'",
    "test:lint": "eslint src --ext .js,.svelte",
    "test": "pnpm test:lint"
  },
  "peerDependencies": {
    "chart.js": "^3.5.0",
    "svelte": "^3.45.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@commitlint/cz-commitlint": "^17.0.0",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "chart.js": "^3.5.0",
    "clean-publish": "^4.0.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "nano-staged": "^0.8.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "^2.75.7",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^5.1.2",
    "standard-version": "^9.5.0",
    "svelte": "^3.45.0"
  }
}
